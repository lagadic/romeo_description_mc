<!--     <param name="outer_diameter" value="0.025"/>
    <param name="inner_diameter" value="0.01250"/> -->
<!--     <param name="outer_diameter" value="0.122"/>
    <param name="inner_diameter" value="0.050"/> -->

    <!-- uncomment when already performed 3D->2D calibration -->
    <!-- 
      <param name="xscale" value="1"/>
      <param name="yscale" value="1"/>
      <param name="axis" value="axis/>
    --> 
  </node>
</launch>
  
