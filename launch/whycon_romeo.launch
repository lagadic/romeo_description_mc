<launch>
  <group ns="camera">
    <node pkg="image_proc" type="image_proc" name="image_proc"/>
  </group>

   <node name="whycon" type="whycon" pkg="whycon"  output="screen">
    <param name="targets" value="1"/> 
    <param name="max_refine" value="5"/>
